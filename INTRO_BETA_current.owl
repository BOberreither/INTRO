    <!-- http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R7_is_initial_element -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R7_is_initial_element">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R5_is_appurtenant_element"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R7_is_representative_element -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R7_is_representative_element">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R5_is_appurtenant_element"/>
    </owl:ObjectProperty>
    


