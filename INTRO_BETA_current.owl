    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R6_has_initial_element -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R6_has_initial_element">
