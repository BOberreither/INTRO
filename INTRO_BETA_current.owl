        </rdfs:range>
        <rdfs:comment>Links any INT4 Intertextual Relationship to a text (on any ontological level) or INT5 Actualization of Feature that is part of the INT4 Intertextual Relationship.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R24_is_related_entity -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R24_is_related_entity">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://erlangen-crm.org/efrbroo/F1_Work"/>
                    <rdf:Description rdf:about="http://erlangen-crm.org/efrbroo/F2_Expression"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT3_Architextual_Entity"/>
