<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#"
     xml:base="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO"
