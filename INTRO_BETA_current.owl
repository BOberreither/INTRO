     -->

    


    <!-- http://purl.org/dc/elements/1.1/description -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/description"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R10_has_Text_Passage -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R10_has_Text_Passage">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R10_is_Text_Passage_of"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://erlangen-crm.org/efrbroo/F1_Work"/>
                    <rdf:Description rdf:about="http://erlangen-crm.org/efrbroo/F2_Expression"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT1_Text_Passage"/>
        <rdfs:comment>Links a F24 Publication Expression to a INT1 Text passage from that F24 Publication Expression, e.g. the book with the ISBN 978-0307278630 to a passage on page three of this book.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R10_is_Text_Passage_of -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R10_is_Text_Passage_of">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT1_Text_Passage"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://erlangen-crm.org/efrbroo/F1_Work"/>
                    <rdf:Description rdf:about="http://erlangen-crm.org/efrbroo/F2_Expression"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment>Links an INT1 Text Passage from a F24 Publication Expression  to that F24 Publication Expression, e.g. a passage on page three of the book with the ISBN 978-0307278630 to that book.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R11_has_fragment -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R11_has_fragment">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R11_is_fragment_of"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT1_Text_Passage"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT1_Text_Passage"/>
        <rdfs:comment>Links a INT1 Text Passage to shorter INT1 Text Passages it contains.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R11_is_fragment_of -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R11_is_fragment_of">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT1_Text_Passage"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT1_Text_Passage"/>
        <rdfs:comment>Links a INT1 Text Passage to a larger INT1 Text Passage it is a part of.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R12_has_referred_to_entity -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R12_has_referred_to_entity">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R24_has_related_entity"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R12_is_referred_to_entity"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT4_Intertextual_Relationship"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://erlangen-crm.org/efrbroo/F1_Work"/>
                    <rdf:Description rdf:about="http://erlangen-crm.org/efrbroo/F2_Expression"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT3_Architextual_Entity"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT5_Actualization_of_Feature"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R12_is_referred_to_entity -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R12_is_referred_to_entity">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R24_is_related_entity"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://erlangen-crm.org/efrbroo/F1_Work"/>
                    <rdf:Description rdf:about="http://erlangen-crm.org/efrbroo/F2_Expression"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT3_Architextual_Entity"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT5_Actualization_of_Feature"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT4_Intertextual_Relationship"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R13_has_referring_entity -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R13_has_referring_entity">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R24_has_related_entity"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R13_is_referring_entity"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT4_Intertextual_Relationship"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://erlangen-crm.org/efrbroo/F1_Work"/>
                    <rdf:Description rdf:about="http://erlangen-crm.org/efrbroo/F2_Expression"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT3_Architextual_Entity"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT5_Actualization_of_Feature"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment>Links an INT4 Reference to a referring entity (a text on any ontological level).</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R13_is_referring_entity -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R13_is_referring_entity">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R24_is_related_entity"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://erlangen-crm.org/efrbroo/F1_Work"/>
                    <rdf:Description rdf:about="http://erlangen-crm.org/efrbroo/F2_Expression"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT3_Architextual_Entity"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT5_Actualization_of_Feature"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT4_Intertextual_Relationship"/>
        <rdfs:comment>Links the referring entity (a text on any ontological level) to the INT4 Reference it constitutes.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R14_likeness_supported_by -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R14_likeness_supported_by">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R14_supports_likeness"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT3_Architextual_Entity"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT6_Characteristic"/>
        <rdfs:comment>Links an INT3 Architextual Entity to an INT6 Characteristic that, found on a text, establishes a likeness to the INT3 Architextual Entity. For Example: The appearance of a ghost in a text establishes a likeness to the &apos;Gothic novel&apos;.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R14_supports_likeness -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R14_supports_likeness">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT6_Characteristic"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT3_Architextual_Entity"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R15_non-affiliation_supported_by -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R15_non-affiliation_supported_by">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R15_supports_non-affiliation"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT3_Architextual_Entity"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT6_Characteristic"/>
        <rdfs:comment>Links an INT3 Architextual Entity to a INT6 Characteristic that, found on a text, is a reason to consider the text not an element of the INT3 Architextual Entity. For Example: The appearance of a teenage vampire could be a reason to consider a text not a representative of a &apos;Bildungsroman&apos;.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R15_supports_non-affiliation -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R15_supports_non-affiliation">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT6_Characteristic"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT3_Architextual_Entity"/>
        <rdfs:comment>Links to a INT6 Characteristic, that, found on a text, is a reason to consider the text not an element of a specific INT3 Architextual Entity, to that INT3 Architextual Entity. For example: The appearance of a ghost is a reason not to consider a text a representative of the genre &apos;crime fiction&apos;.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R16_supports_unlikeness -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R16_supports_unlikeness">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R16_unlikeness_supported_by"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT6_Characteristic"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT3_Architextual_Entity"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R16_unlikeness_supported_by -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R16_unlikeness_supported_by">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT3_Architextual_Entity"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT6_Characteristic"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R17_actualizes_feature -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R17_actualizes_feature">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R17_feature_actualized_in"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT5_Actualization_of_Feature"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT2_Receptional_Entity"/>
        <rdfs:comment>Links the specific INT5 Actualization of Feature to the (abstract concept of the) feature it actualizes, a INT2 Receptional Entity.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R17_feature_actualized_in -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R17_feature_actualized_in">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT2_Receptional_Entity"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT5_Actualization_of_Feature"/>
        <rdfs:comment>Links the INT2 Receptional Entity, the feature, to an INT5 Actualization of Feature (that is itself linked to a specific text).</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R18_actualization_found_on -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R18_actualization_found_on">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R18_shows_actualization"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT5_Actualization_of_Feature"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://erlangen-crm.org/efrbroo/F1_Work"/>
                    <rdf:Description rdf:about="http://erlangen-crm.org/efrbroo/F2_Expression"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT3_Architextual_Entity"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT5_Actualization_of_Feature"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R18_shows_actualization -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R18_shows_actualization">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://erlangen-crm.org/efrbroo/F1_Work"/>
                    <rdf:Description rdf:about="http://erlangen-crm.org/efrbroo/F2_Expression"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT3_Architextual_Entity"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT5_Actualization_of_Feature"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT5_Actualization_of_Feature"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R19_has_type -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R19_has_type">
        <rdfs:subPropertyOf rdf:resource="http://erlangen-crm.org/current/P2_has_type"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R19_is_type_of"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT4_Intertextual_Relationship"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT22_Type_of_Intertextual_Relationship"/>
        <rdfs:comment>Links an INT4 Reference to its INT22 Type of Intertextual Relationship.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R19_is_type_of -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R19_is_type_of">
        <rdfs:subPropertyOf rdf:resource="http://erlangen-crm.org/current/P2i_is_type_of"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT22_Type_of_Intertextual_Relationship"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT4_Intertextual_Relationship"/>
        <rdfs:comment>Links a INT22 Type of Intertextual Relationship to the INT4 Reference of that type.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R1_affiliation_supported_by -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R1_affiliation_supported_by">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R1_supports_affiliation_to"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT3_Architextual_Entity"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT6_Characteristic"/>
        <rdfs:comment>Links an INT3 Architextual Entity to a INT6 Characteristic that, found on a text, is a reason to consider the text an element of the INT3 Architextual Entity. For example: The appearance of a ghost is a reason to consider a text a representative of the &apos;Gothic novel&apos;.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R1_supports_affiliation_to -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R1_supports_affiliation_to">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT6_Characteristic"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT3_Architextual_Entity"/>
        <rdfs:comment>Links to an INT3 Architextual Entity a INT6 Characteristic that, if found on a text, is a reason to consider the text an element of the INT3 Architextual Entity. For example: The appearance of a ghost is a reason to consider a text a representative of the &apos;Gothic novel&apos;.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R20_discusses -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R20_discusses">
        <rdfs:subPropertyOf rdf:resource="http://erlangen-crm.org/current/P67_refers_to"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R20_is_discussed_in"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT44_Interpretament"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://erlangen-crm.org/efrbroo/F1_Work"/>
                    <rdf:Description rdf:about="http://erlangen-crm.org/efrbroo/F2_Expression"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT3_Architextual_Entity"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment>Links an INT44 Interpretament to the textual instance (on any ontological level) it discusses.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R20_is_discussed_in -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R20_is_discussed_in">
        <rdfs:subPropertyOf rdf:resource="http://erlangen-crm.org/current/P67i_is_referred_to_by"/>
        <rdfs:domain>
            <owl:Class>
