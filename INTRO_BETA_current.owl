        <rdfs:domain rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT2_Receptional_Entity"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT4_Intertextual_Relationship"/>
        <rdfs:comment>Links a INT2 Receptional Entity to a INT4 Reference it provides the similarity for, the similarity being necessary for the detection of the INT4 Reference. States that an INT4 Reference is based on a similarity, i.e. the occurrence of the same INT2 Receptional Entity in two different texts.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R22_reference_is_based_on_similarity -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R22_reference_is_based_on_similarity">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT4_Intertextual_Relationship"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT2_Receptional_Entity"/>
        <rdfs:comment>Links an INT4 Reference to an INT2 Receptional Entity that provides the similarity necessary for the detection of the INT4 Reference. States that an INT4 Reference is based on a similarity, i.e. the occurrence of the same receptional entity in two different texts.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R23_provides_support_for -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R23_provides_support_for">
