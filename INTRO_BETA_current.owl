        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://erlangen-crm.org/efrbroo/F1_Work"/>
                    <rdf:Description rdf:about="http://erlangen-crm.org/efrbroo/F2_Expression"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT3_Architextual_Entity"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment>Links a transtextual entity to a text (on any ontological level) that is an element of the transtextual entity.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R5_is_appurtenant_element -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#R5_is_appurtenant_element">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://erlangen-crm.org/efrbroo/F1_Work"/>
                    <rdf:Description rdf:about="http://erlangen-crm.org/efrbroo/F2_Expression"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT3_Architextual_Entity"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.semanticweb.org/oberreb4/ontologies/2018/9/INTRO#INT3_Architextual_Entity"/>
