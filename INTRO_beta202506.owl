	<owl:Class rdf:about="http://www.cidoc-crm.org/cidoc-crm/E73_Information_Object"/>



	<!-- http://www.cidoc-crm.org/cidoc-crm/E89_Propositional_Object -->

	<owl:Class rdf:about="http://www.cidoc-crm.org/cidoc-crm/E89_Propositional_Object"/>



	<!-- http://www.cidoc-crm.org/cidoc-crm/E90_Symbolic_Object -->

	<owl:Class rdf:about="http://www.cidoc-crm.org/cidoc-crm/E90_Symbolic_Object"/>



