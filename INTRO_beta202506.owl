		<rdfs:domain rdf:resource="https://w3id.org/lso/intro/beta202506#INT4_Feature"/>
		<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
		<rdfs:comment xml:lang="en">Links a feature to a data string defining or describing it more closely.</rdfs:comment>
		<rdfs:label xml:lang="en">R43 has specification</rdfs:label>
	</owl:DatatypeProperty>



	<!-- https://w3id.org/lso/intro/beta202506#R44_hasWording -->

	<owl:DatatypeProperty rdf:about="https://w3id.org/lso/intro/beta202506#R44_hasWording">
