            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://erlangen-crm.org/efrbroo/F1_Work"/>
                    <rdf:Description rdf:about="http://erlangen-crm.org/efrbroo/F2_Expression"/>
