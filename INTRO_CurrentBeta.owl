    <owl:ObjectProperty rdf:about="https://w3id.org/lso/intro/currentbeta#R10_has_Text_Passage">
        <owl:inverseOf rdf:resource="https://w3id.org/lso/intro/currentbeta#R10_is_Text_Passage_of"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://erlangen-crm.org/efrbroo/F1_Work"/>
                    <rdf:Description rdf:about="http://erlangen-crm.org/efrbroo/F2_Expression"/>
