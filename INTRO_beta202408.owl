                    <rdf:Description rdf:about="http://iflastandards.info/ns/lrm/lrmoo/F1_Work"/>
                    <rdf:Description rdf:about="http://www.cidoc-crm.org/cidoc-crm/E73_Information_Object"/>
                    <rdf:Description rdf:about="https://w3id.org/lso/intro/beta202304#INT6_ArchitextualEntity"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Links an INT7 Interpretament to the textual instance (on any ontological level) it discusses.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/lso/intro/beta202304#R20i_isDiscussedIn -->

    <owl:ObjectProperty rdf:about="https://w3id.org/lso/intro/beta202304#R20i_isDiscussedIn">
        <rdfs:subPropertyOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/P67i_is_referred_to_by"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://iflastandards.info/ns/lrm/lrmoo/F1_Work"/>
                    <rdf:Description rdf:about="http://www.cidoc-crm.org/cidoc-crm/E73_Information_Object"/>
                    <rdf:Description rdf:about="https://w3id.org/lso/intro/beta202304#INT6_ArchitextualEntity"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/lso/intro/beta202304#INT_Interpretation"/>
        <rdfs:comment xml:lang="en">Links a textual instance (on any ontological level) to the INT7 Interpretament it is discussed in.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/lso/intro/beta202304#R21_identifies -->

    <owl:ObjectProperty rdf:about="https://w3id.org/lso/intro/beta202304#R21_identifies">
        <rdfs:subPropertyOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/P67_refers_to"/>
        <owl:inverseOf rdf:resource="https://w3id.org/lso/intro/beta202304#R21i_isIdentifiedBy"/>
        <rdfs:domain rdf:resource="https://w3id.org/lso/intro/beta202304#INT_Interpretation"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/lso/intro/beta202304#INT2_ActualizationOfFeature"/>
                    <rdf:Description rdf:about="https://w3id.org/lso/intro/beta202304#INT3_Interrelation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Links an INT7 Interpretament to the INT5 Characteristic, the INT3 Intertextual Relationship or the INT2 Actualization of Feature it identifies.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/lso/intro/beta202304#R21i_isIdentifiedBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/lso/intro/beta202304#R21i_isIdentifiedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/P67i_is_referred_to_by"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/lso/intro/beta202304#INT2_ActualizationOfFeature"/>
                    <rdf:Description rdf:about="https://w3id.org/lso/intro/beta202304#INT3_Interrelation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/lso/intro/beta202304#INT_Interpretation"/>
        <rdfs:comment xml:lang="en">Links an INT3 Intertextual Relationship or an INT2 Actualization of Featurea to the INT7 Interpretament it is identified in.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/lso/intro/beta202304#R22_providesSimilarityForReference -->

    <owl:ObjectProperty rdf:about="https://w3id.org/lso/intro/beta202304#R22_providesSimilarityForReference">
        <owl:inverseOf rdf:resource="https://w3id.org/lso/intro/beta202304#R22i_referenceIsBasedOnSimilarity"/>
        <rdfs:domain rdf:resource="https://w3id.org/lso/intro/beta202304#INT4_ReceptionalEntity"/>
        <rdfs:range rdf:resource="https://w3id.org/lso/intro/beta202304#INT3_Interrelation"/>
        <rdfs:comment xml:lang="en">Links an INT4 Receptional Entity to a INT3 Intertextual Relationship it provides the similarity for, the similarity being necessary for the detection of the INT3 Intertextual Relationship. States that an INT3 Intertextual Relationship is based on a similarity, i.e. the
			actualization of the same INT4 Receptional Entity in two different texts.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/lso/intro/beta202304#R22i_referenceIsBasedOnSimilarity -->

    <owl:ObjectProperty rdf:about="https://w3id.org/lso/intro/beta202304#R22i_referenceIsBasedOnSimilarity">
        <rdfs:domain rdf:resource="https://w3id.org/lso/intro/beta202304#INT3_Interrelation"/>
        <rdfs:range rdf:resource="https://w3id.org/lso/intro/beta202304#INT4_ReceptionalEntity"/>
        <rdfs:comment xml:lang="en">Links an INT3 Intertextual Relationship to an INT4 Receptional Entity that provides the similarity necessary for the detection of the INT3 Intertextual Relationship. States that an INT3 Intertextual Relationship is based on a similarity, i.e. the actualization of the same
			receptional entity in two different texts.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/lso/intro/beta202304#R23_providesSupportFor -->

    <owl:ObjectProperty rdf:about="https://w3id.org/lso/intro/beta202304#R23_providesSupportFor">
        <owl:inverseOf rdf:resource="https://w3id.org/lso/intro/beta202304#R23i_supportProvidedBy"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://iflastandards.info/ns/lrm/lrmoo/F1_Work"/>
                    <rdf:Description rdf:about="http://www.cidoc-crm.org/cidoc-crm/E73_Information_Object"/>
                    <rdf:Description rdf:about="https://w3id.org/lso/intro/beta202304#INT1_Passage"/>
                    <rdf:Description rdf:about="https://w3id.org/lso/intro/beta202304#INT2_ActualizationOfFeature"/>
                    <rdf:Description rdf:about="https://w3id.org/lso/intro/beta202304#INT6_ArchitextualEntity"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/lso/intro/beta202304#INT2_ActualizationOfFeature"/>
                    <rdf:Description rdf:about="https://w3id.org/lso/intro/beta202304#INT3_Interrelation"/>
                    <rdf:Description rdf:about="https://w3id.org/lso/intro/beta202304#INT4_ReceptionalEntity"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Relates a source (a text on any ontological level or an actualization of a feature on said source) to the outcome of a receptional or interpretative act (an INT2 Actualization of Feature, a INT4 Receptional Entity like an INT Interpretation, an INT3 Intertextual Relationship) to support it.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/lso/intro/beta202304#R23i_supportProvidedBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/lso/intro/beta202304#R23i_supportProvidedBy">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/lso/intro/beta202304#INT2_ActualizationOfFeature"/>
                    <rdf:Description rdf:about="https://w3id.org/lso/intro/beta202304#INT3_Interrelation"/>
                    <rdf:Description rdf:about="https://w3id.org/lso/intro/beta202304#INT4_ReceptionalEntity"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://iflastandards.info/ns/lrm/lrmoo/F1_Work"/>
                    <rdf:Description rdf:about="http://www.cidoc-crm.org/cidoc-crm/E73_Information_Object"/>
                    <rdf:Description rdf:about="https://w3id.org/lso/intro/beta202304#INT1_Passage"/>
                    <rdf:Description rdf:about="https://w3id.org/lso/intro/beta202304#INT2_ActualizationOfFeature"/>
                    <rdf:Description rdf:about="https://w3id.org/lso/intro/beta202304#INT6_ArchitextualEntity"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Relates the decision made in any receptional or interpretative act - an INT2 Actualization of Feature, a INT4 Receptional Entity like an INT Interpretation, an INT3 Intertextual Relationship - to a source that can support this decision (a text on any ontological level or one of its features).</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/lso/intro/beta202304#R24_hasRelatedEntity -->

    <owl:ObjectProperty rdf:about="https://w3id.org/lso/intro/beta202304#R24_hasRelatedEntity">
        <owl:inverseOf rdf:resource="https://w3id.org/lso/intro/beta202304#R24i_isRelatedEntity"/>
        <rdfs:domain rdf:resource="https://w3id.org/lso/intro/beta202304#INT3_Interrelation"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://iflastandards.info/ns/lrm/lrmoo/F1_Work"/>
                    <rdf:Description rdf:about="http://www.cidoc-crm.org/cidoc-crm/E73_Information_Object"/>
                    <rdf:Description rdf:about="https://w3id.org/lso/intro/beta202304#INT1_Passage"/>
                    <rdf:Description rdf:about="https://w3id.org/lso/intro/beta202304#INT2_ActualizationOfFeature"/>
                    <rdf:Description rdf:about="https://w3id.org/lso/intro/beta202304#INT6_ArchitextualEntity"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Links any INT3 Intertextual Relationship to a text (on any ontological level) or INT2 Actualization of Feature that is part of the INT3 Intertextual Relationship.
The skos:boradMatch states that this property is a more specific version of oa:hasTarget in the sense that the INT3 Intertextual Relationship can be seen as an annotation, the texts linked by this Relationship as their targets.</rdfs:comment>
        <skos:broadMatch rdf:resource="http://www.w3.org/ns/oa#hasTarget"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/lso/intro/beta202304#R24i_isRelatedEntity -->

    <owl:ObjectProperty rdf:about="https://w3id.org/lso/intro/beta202304#R24i_isRelatedEntity">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://iflastandards.info/ns/lrm/lrmoo/F1_Work"/>
                    <rdf:Description rdf:about="http://www.cidoc-crm.org/cidoc-crm/E73_Information_Object"/>
                    <rdf:Description rdf:about="https://w3id.org/lso/intro/beta202304#INT1_Passage"/>
                    <rdf:Description rdf:about="https://w3id.org/lso/intro/beta202304#INT2_ActualizationOfFeature"/>
                    <rdf:Description rdf:about="https://w3id.org/lso/intro/beta202304#INT6_ArchitextualEntity"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/lso/intro/beta202304#INT3_Interrelation"/>
        <rdfs:comment xml:lang="en">Links any text (on any ontological level) or INT2 Actualization of Feature to the INT3 Intertextual Relationship it is a part of.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/lso/intro/beta202304#R25_hasSegment -->

    <owl:ObjectProperty rdf:about="https://w3id.org/lso/intro/beta202304#R25_hasSegment">
        <owl:inverseOf rdf:resource="https://w3id.org/lso/intro/beta202304#R25i_isSegmentOf"/>
        <rdfs:domain rdf:resource="http://iflastandards.info/ns/lrm/lrmoo/F3_Manifestation"/>
        <rdfs:range rdf:resource="https://w3id.org/lso/intro/beta202304#INT16_Segment"/>
        <rdfs:comment xml:lang="en">Links a F24 Publication Expression to a INT16 Segment it contains (the segment in turn R16 incorporates an INT1 Text Passage or an F22 Expression).</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/lso/intro/beta202304#R25i_isSegmentOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/lso/intro/beta202304#R25i_isSegmentOf">
        <rdfs:domain rdf:resource="https://w3id.org/lso/intro/beta202304#INT16_Segment"/>
        <rdfs:range rdf:resource="http://iflastandards.info/ns/lrm/lrmoo/F3_Manifestation"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/lso/intro/beta202304#R3_constellates -->

    <owl:ObjectProperty rdf:about="https://w3id.org/lso/intro/beta202304#R3_constellates">
        <owl:inverseOf rdf:resource="https://w3id.org/lso/intro/beta202304#R3i_isConstellatedBy"/>
        <rdfs:domain rdf:resource="https://w3id.org/lso/intro/beta202304#INT_Constellation"/>
        <rdfs:range rdf:resource="https://w3id.org/lso/intro/beta202304#INT2_ActualizationOfFeature"/>
        <rdfs:comment xml:lang="en">Links an INT Constellation to the elements (INT2 Actualizations of Feature) that together form that INT Constellation.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/lso/intro/beta202304#R3i_isConstellatedBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/lso/intro/beta202304#R3i_isConstellatedBy">
        <rdfs:domain rdf:resource="https://w3id.org/lso/intro/beta202304#INT2_ActualizationOfFeature"/>
        <rdfs:range rdf:resource="https://w3id.org/lso/intro/beta202304#INT_Constellation"/>
        <rdfs:comment xml:lang="en">Links INT2 Actualizations of Feature of at least two INT4 Receptional Entitiess with the INT Constellation they form (i.e. stating that they do so).</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/lso/intro/beta202304#R4_defines -->

    <owl:ObjectProperty rdf:about="https://w3id.org/lso/intro/beta202304#R4_defines">
        <owl:inverseOf rdf:resource="https://w3id.org/lso/intro/beta202304#R4i_isDefinedIn"/>
        <rdfs:domain rdf:resource="https://w3id.org/lso/intro/beta202304#INT2_ActualizationOfFeature"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/lso/intro/beta202304#INT11_TypeOfInterrelation"/>
                    <rdf:Description rdf:about="https://w3id.org/lso/intro/beta202304#INT4_ReceptionalEntity"/>
                    <rdf:Description rdf:about="https://w3id.org/lso/intro/beta202304#INT6_ArchitextualEntity"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Relates an actualization (usually: of the semantic feature &apos;INT Definition&apos;) to a type of intertextual relationship and to receptional and architextual entities and thereby links a concept - e.g. &apos;discourse&apos; - to a definition - e.g. a text passage in
			Foucault&apos;s writings.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/lso/intro/beta202304#R4i_isDefinedIn -->

    <owl:ObjectProperty rdf:about="https://w3id.org/lso/intro/beta202304#R4i_isDefinedIn">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/lso/intro/beta202304#INT11_TypeOfInterrelation"/>
                    <rdf:Description rdf:about="https://w3id.org/lso/intro/beta202304#INT4_ReceptionalEntity"/>
                    <rdf:Description rdf:about="https://w3id.org/lso/intro/beta202304#INT6_ArchitextualEntity"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/lso/intro/beta202304#INT2_ActualizationOfFeature"/>
        <rdfs:comment xml:lang="en">Relates an INT11 Type of Intertextual Relationship or an INT4 Receptional, or INT6 Architextual Entity to the INT2 Actualization of a Feature (usually: of the semantic feature &apos;definition&apos;) in order to link a definition - e.g. a text passage in Foucault&apos;s
			writings - to the concept it defines - e.g. &apos;discourse&apos;.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/lso/intro/beta202304#R8_hasOpinionObject -->

    <owl:ObjectProperty rdf:about="https://w3id.org/lso/intro/beta202304#R8_hasOpinionObject">
        <rdfs:subPropertyOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/P67i_is_referred_to_by"/>
        <owl:inverseOf rdf:resource="https://w3id.org/lso/intro/beta202304#R8i_isOpinionObjectOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/lso/intro/beta202304#INT_Opinion"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment xml:lang="en">Links an INT Opinion to its object - for example an opinion with the theme fashion (to make it an opinion about fashion).</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/lso/intro/beta202304#R8i_isOpinionObjectOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/lso/intro/beta202304#R8i_isOpinionObjectOf">
        <rdfs:subPropertyOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/P67_refers_to"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="https://w3id.org/lso/intro/beta202304#INT_Opinion"/>
        <rdfs:comment xml:lang="en">Links an object with an INT Opinion about it.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/lso/intro/beta202304#R9_hasSubsequentInterpretament -->

    <owl:ObjectProperty rdf:about="https://w3id.org/lso/intro/beta202304#R9_hasSubsequentInterpretament">
        <owl:inverseOf rdf:resource="https://w3id.org/lso/intro/beta202304#R9i_hasPrecedingInterpretament"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/lso/intro/beta202304#INT_Interpretation"/>
        <rdfs:range rdf:resource="https://w3id.org/lso/intro/beta202304#INT_Interpretation"/>
        <rdfs:comment xml:lang="en">Links an INT7 Interpretamet to another INT7 Interpretament following it (logically or chronologically).</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/lso/intro/beta202304#R9i_hasPrecedingInterpretament -->

    <owl:ObjectProperty rdf:about="https://w3id.org/lso/intro/beta202304#R9i_hasPrecedingInterpretament">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/lso/intro/beta202304#INT_Interpretation"/>
        <rdfs:range rdf:resource="https://w3id.org/lso/intro/beta202304#INT_Interpretation"/>
        <rdfs:comment xml:lang="en">Links an INT7 Interpretamet to another INT7 Interpretament preceding it (logically or chronologically).</rdfs:comment>
    </owl:ObjectProperty>
    


